From 97bdf11f63d36bc53402d7eac3e9e6cd2bea2fd3 Mon Sep 17 00:00:00 2001
From: Gleb Mazovetskiy <glex.spb@gmail.com>
Date: Wed, 8 Apr 2020 19:53:45 +0100
Subject: [PATCH] Remove -mframe-header-opt

Not supported on GCC 5.5
Everything else is supported
---
 Makefile.gcw0 | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/Makefile.gcw0 b/Makefile.gcw0
index 363c3a1..4afc29b 100644
--- a/Makefile.gcw0
+++ b/Makefile.gcw0
@@ -41,7 +41,7 @@ CFLAGS = $(C_ARCH) -O2 -DGCW_ZERO \
 	$(SDL_CFLAGS)
 	
 ifdef GCC_NEW
-CFLAGS += -mno-abicalls -mframe-header-opt -fno-PIC -mno-check-zero-division -flto
+CFLAGS += -mno-abicalls -fno-PIC -mno-check-zero-division -flto
 endif
 
 CFLAGS += -DUSE_BGR15 -DRUMBLE -DCHEAT_UNZIP -DHW_SCALE
